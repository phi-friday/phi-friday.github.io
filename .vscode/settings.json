{
  "css.customData": [
    ".vscode/tailwind.json"
  ],
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.tabSize": 2,
  "css.lint.unknownAtRules": "ignore",
  "eslint.useFlatConfig": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.formatOnSave": true,
}